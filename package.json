{
  "name": "ember-cli-notifications",
  "version": "4.2.4",
  "description": "Atom inspired notification messages for ember-cli",
  "keywords": [
    "ember-addon",
    "ember",
    "ember-cli",
    "notifications",
    "alerts"
  ],
  "license": "MIT",
  "author": "Jonathan Steele",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": {
